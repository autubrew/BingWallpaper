$(#btnScan1).on("click", function() {
    view.selectCustomDir("wpdir")
})

$(#btnScan2).on("click", function() {
    view.selectCustomDir("likedir")
})

$(#btnOk).on("click", function() {
    var v1 = self.$(input[id="wpdir"]).value
    var v2 = self.$(input[id="likedir"]).value
    view.ok(v1, v2)
    view.close()
})

$(#btnCancel).on("click", function() {
    view.close()
})
